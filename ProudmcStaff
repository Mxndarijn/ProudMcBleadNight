	public void zombi(EntityDamageByEntityEvent Event) {
		if(Event.getEntity().getType().equals(EntityType.ZOMBIE)) {
			Zombie zomb = (Zombie) Event.getEntity();
			String zombb = zomb.getCustomName();
			if(zombb.contains("Bandieten")) {
				if(Event.getDamager() instanceof Player) {
					Double damage  = Event.getDamage();
					zomb.setHealth(zomb.getHealth() - damage);
					
					zomb.setCustomName("ยง Bandieten  "  + zomb.getHealth() );
					Event.setCancelled(true);
			}
              
				
			}
		}
	}
